    "rollup-plugin-stylelint": "1.0.0",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.34.1",
    "sass": "1.56.1",
    "sass-loader": "13.2.0",
    "style-loader": "3.3.1",
    "stylelint": "14.15.0",
    "stylelint-order": "5.0.0",
    "stylelint-scss": "4.3.0",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.1",
    "typescript": "4.8.4",
    "webpack": "5.75.0"
  },
  "dependencies": {
    "classnames": "2.3.2",
    "date-fns": "2.29.3",
    "date-fns-tz": "1.3.7",
    "react-textarea-autosize": "8.4.0",
    "uuid": "9.0.0"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
